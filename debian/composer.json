{
    "name": "flexipeehp",
    "description": "PHP Based Library for easy interaction with czech accounting system Flexibee.",
    "homepage": "https://github.com/Spoje-NET/FlexiPeeHP",
    "license": "GPL-2.0+",
    "version": "1.11",
    "type": "library",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "vitex@arachne.cz"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "ext-curl": "*",
        "ease-framework": "*"
    },
    "autoload": {
        "psr-4": {
            "FlexiPeeHP\\": "."
        }
    },
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/Ease",
            "options": {
                "symlink": true
            }
        }
    ]
}
